@model List<UI.Models.Players.PlayerAchievementWinnerViewModel>
    @{
    Layout = null;
    }

    <div id="gamingGroupsVote" class="fl-right" style="margin-top: 21px; margin-bottom: 10.5px;"></div>
    <div class="fl-clear"></div>
    <div class="flip-scroll">
        <table class="table table-hover table-striped" id="topGamingGroupsTable">
            <thead>
            <tr>
                <th>Achievement</th>
                <th>Player</th>
                <th>Gaming Group</th>
                <th>Date</th>
            </tr>
            </thead>
            @{ var position = 1; }
            @foreach (var playerAchievement in Model)
            {
            <tr data-position="@position">
                @{ position++; }
                <td>@playerAchievement.Achievement.Name</td>
            </tr>
            }
        </table>
    </div>
