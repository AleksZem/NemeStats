@model UI.Models.GamingGroup.GamingGroupsSummaryViewModel
@{
	ViewBag.Title = "Top " + Model.GamingGroups.Count + " Gaming Groups";
    ViewBag.Description = "See the top Gaming Groups on NemeStats to check out some of the cool stats you can get if you record your games!";
	Layout = MVC.Shared.Views._BaseLayout;
}

<div class="container">
    <h2 class="page-header">Top @Model.GamingGroups.Count Gaming Groups</h2>
	@Html.Partial(MVC.GamingGroup.Views._TopGamingGroupsPartial, Model)
</div>
